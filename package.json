{
  "name": "@brr-dev/contentful",
  "version": "0.1.1",
  "description": "A basic Typescript SDK for the Contentful Delivery API.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist"
  ],
  "repository": "https://github.com/brr-dev/contentful",
  "author": "Brandon Ramirez <brandon@brr.dev>",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "rm -rf dist && tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "yarn build",
    "prepublishOnly": "yarn format && yarn test",
    "version": "yarn format && git add -A src",
    "postVersion": "git push && git push --tags"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@brr-dev/api": "^1.0.6"
  }
}
